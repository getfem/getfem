    mfile.write('% ' + oname + '/subsref\n')
    mfile.write('  obj=struct(obj);\n')
    mfile.write('  nout = max(nargout, 1); cnt=1;\n')
    mfile.write('  FGET = @gf_'+o+'_get;\n')

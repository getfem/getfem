    mfile.write('      else\n')
    mfile.write('	 m = gf_'+o+'(a,varargin{:});\n')
    mfile.write('      end;\n')
    mfile.write('    else\n')
    mfile.write('      m = gf_'+o+'(a,varargin{:});\n')
    mfile.write('    end;\n')
    mfile.write('    m.txt = \'\';\n')
    mfile.write('    m = class(m, this_class);\n')
    mfile.write('  end;\n')
    

          sub_com.add(mname)
          [ok, doc, dtype, mname, params, ret] = ExtractSubDoc(fl, language)
        fl.close()

  # Build directory and files for Matlab OO-commands
  for o in objects:

    [oname, I] = StandardObjectName(o)

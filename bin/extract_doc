        mfile.write('else\n')
        mfile.write('  gf_'+o+'_'+ext+'(struct(obj),varargin{:});\n')
        mfile.write('  if (exist(\'ans\',\'var\') == 1), varargout{1}=ans;\n')
        mfile.write('  end;\n')
        mfile.write('end;\n')
        mfile.write('% autogenerated mfile;\n')
        mfile.close() 




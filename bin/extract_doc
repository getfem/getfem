          nbspace = -1; util_pos = -1; nbsp = -1
          for l in doc.split('\n'):
            if (nbspace == -1 and len(l.strip()) > 0):
              nbspace = 0;
              for i in range(len(l)):
                if (l[i] == '\t'):
                    nbspace = (nbspace/8)*8 + 8;
                elif (l[i].isspace()):
                    nbspace = nbspace + 1;
                else: break
            
            if (nbspace > 0):
              util_pos = 0; nbsp = 0
              for i in range(len(l)):
                if (l[i] == '\t'):
                    nbsp = (nbsp/8)*8 + 8;
                elif (l[i].isspace()):
                    nbsp = nbsp + 1;
                else: break
                util_pos = i + 1
                if (nbsp >= nbspace): break
                    
              l = l[util_pos:]
              
            if (nbsp > nbspace):
                for i in range (nbsp-nbspace):
                    print(''),
            
            if (nbspace >= 0):
                print('    '+l)
                
          print('')
          [ok, doc, dtype, mname, params, ret] = ExtractSubDoc(fl, language)
    
        fl.close()


